{
  "extends": "tslint:recommended",
  "rules": {
    "no-console": false,
    "trailing-comma": false,
    "no-shadowed-variable": false,
    "typedef": [
      true,
      "call-signature",
      "property-declaration",
      "arrow-parameter",
      "parameter",
    ],
    "no-inferrable-types": true,
    "max-line-length": false,
    "object-literal-sort-keys": false,
    "ordered-imports": false
  }
}
